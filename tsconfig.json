{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "allowJs": true,
    "outDir": "./dist/",
    "jsx": "react",
    "noImplicitThis": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "lib": [
      "DOM",
      "es2015",
      "es2016",
      "es2017",
      "es2019",
      "webworker"
    ]
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
